
  <div class="modal-header">
    <h1 class="modal-title"><%= wishlist.title %></h1>
    <button type="button" class="close" data-dismiss="modal"><span>&times;</span></button>
  </div>
  <h2><%= wishlist.createdAt.toISOString().slice(0, 10) %></h2>

  <div class="modal-body">
    <%- include('../_partials/wishlists/form', { action: `/wishlists/${wishlist.id}?_method=PUT` }) %>
  </div>

  <div class="d-flex justify-content-between modal-footer">
    <button class="btn btn-warning" type="button" data-dismiss="modal">Close</button>
    <button id="wishlist-form-submit" type="button" class="btn btn-success" data-url="/wishlists/<%= wishlist.id %> " data-method="PUT">Submit</button>
  </div>
